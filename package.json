{
  "name": "repromodel",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "check-venv": "if [ ! -d \".venv\" ]; then python3 -m venv .venv; fi",
    "install-deps": "npm install && ./.venv/bin/pip install -r repromodel_core/requirements.txt",
    "generate-choices": "./.venv/bin/python3 repromodel_core/generate_choices.py",
    "repromodel": "npm run check-venv && npm run install-deps && npm run generate-choices && concurrently \"npm run dev-frontend\" \"npm run dev-backend\"",
    "dev-frontend": "npm run dev",
    "dev-backend": "source ./.venv/bin/activate && python3 repromodel_core/app.py",
    "repromodel-with-llm": "concurrently \"ollama run llama3\" \"npm run repromodel\""
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.15.19",
    "@mui/material": "^5.15.16",
    "axios": "^1.6.8",
    "dedent": "^1.5.3",
    "formik": "^2.4.6",
    "prismjs": "^1.29.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-select": "^5.8.0",
    "react-simple-code-editor": "^0.13.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "vite": "^5.2.0",
    "wait-on": "^7.2.0"
  }
}
