{
    "models": ["ConvAE_2D"],
    "models_params": [{
        "n_layers": 4,
        "input_channels": 3,
        "output_channels": 2,
        "activation": "softmax"
    }],
    "preprocessing": "DummyPreprocessor",
    "preprocessing_params": {
      "parent_input_path": "repromodel_core/data/dummyData",
      "parent_output_path": "repromodel_core/data/dummyData_preprocessed",
      "num_workers": 10
    },
    "augmentations": "ShiftScaleRotateFlip",
    "augmentations_params": {
      "p": 0.5,
      "shift_limit": 0.2,
      "scale_limit": 0.2,
      "rotate_limit": 60
    },
    "datasets": "DummyDataset",
    "datasets_params": {
      "input_path": "repromodel_core/data/dummyData_preprocessed/input",
      "target_path": "repromodel_core/data/dummyData_preprocessed/target",
      "extension": ".npy"
    },
    "optimizers": "Adam",
    "optimizers_params": {
      "lr": 0.001,
      "betas": [0.9, 0.999],
      "eps": 1e-08,
      "weight_decay": 0,
      "amsgrad": false
    },
    "lr_schedulers": "ReduceLROnPlateau",
    "lr_schedulers_params": {"factor": 0.5,
                            "patience": 10
    },
    "losses": "BCELoss",
    "losses_params": {},
    "early_stopping": "ScheduledStop",
    "early_stopping_params": {
      "max_epochs": 10
    },
    "metrics": ["DiceScore"],
    "metrics_params": [{
      "threshold": 0.5,
      "smooth": 0.001  
    }],
    
    "device": "cpu",
    "batch_size": 8,
    "monitor": "val_loss",
    "data_splits": {
      "k": 5,
      "random_seed": 42
    },
    "model_save_path": "repromodel_core/ckpts",
    "tensorboard_log_path": "repromodel_core/logs",
    "metadata_path": "repromodel_core/logs/metadata.json",
    "load_from_checkpoint": true
  }